{
  "name": "@skhaz/zstd",
  "version": "1.0.9",
  "description": "Zstd binding for Nodejs, with TypeScript support.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "install": "node-gyp-build",
    "test": "nyc mocha -t 5000",
    "build:gyp:dev": "node-gyp -j 8 build --debug",
    "build:gyp": "node-gyp -j 8 build",
    "build:ts": "tsc -p .",
    "build": "npm run build:gyp && npm run build:ts",
    "rebuild:dev": "node-gyp -j 8 rebuild --debug",
    "rebuild": "node-gyp -j 8 rebuild",
    "prebuildify": "prebuildify -t node@ --strip",
    "clean": "node-gyp clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skhaz/zstd.git"
  },
  "keywords": [
    "zstd",
    "compression",
    "decompression"
  ],
  "author": "zwb-ict",
  "contributors": [
