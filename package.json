    }
  ],
  "license": "MIT",
  "dependencies": {
    "nan": "^2.14.2",
    "node-gyp-build": "^4.3.0",
    "pond": "^2.1.2"
  },
  "devDependencies": {
    "@types/bindings": "^1.5.1",
    "@types/node": "^16.11.6",
    "chai": "^4.2.0",
    "mocha": "^8.2.0",
    "nyc": "^15.1.0",
    "prebuildify": "^4.2.1",
    "typescript": "^4.4.4"
  }
}
